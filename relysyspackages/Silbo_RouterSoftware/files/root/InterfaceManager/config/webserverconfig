
config webserverconfig 'webserverconfig'
	option port '80'
	option enablentpsync '1'
	option ntpsyncinterval '15'
	option https_port '443'
	option rfc1918_filter '1'
	option http_port '80'
 	option enable_http '1'
	option enable_https '1'
    option redirect_https '0'
    option sessiontimeout '5'
    list ntpserver '0.openwrt.pool.ntp.org'
	list ntpserver '1.openwrt.pool.ntp.org'
	list ntpserver '2.openwrt.pool.ntp.org'
	list ntpserver '3.openwrt.pool.ntp.org'
