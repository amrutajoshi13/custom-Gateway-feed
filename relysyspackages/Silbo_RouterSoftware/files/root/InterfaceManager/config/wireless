
config wifi-device 'ra0'
	option type 'ralink'
	option variant 'mt7628'
	option hwmode '11g'
	option htmode 'HT40'
	option disabled '0'
	option country 'IN'

config wifi-iface 'ap'
	option device 'ra0'
	option mode 'ap'
	option network 'lan'
	option ifname 'ra0'
	option disabled '0'
	option key '12345678'
	option encryption 'AES'
	option ssid 'Silbo_RC44'

config wifi-iface 'sta'
	option device 'ra0'
	option mode 'sta'
	option network 'WIFI_WAN'
	option ifname 'apcli0'
	option disabled '1'
	option ssid 'ApClient'
	option key '12345678'
	option encryption 'mixed-psk'

