config global 
	option networkModeLed '1'
	option signalStrengthLed '1'
	option numberOfModems '1'
	option numberOfLimits '3'
	
config gpio 'modem1'
	option signalStrengthIndicatorType 'ledstrip'
	option numberOfSignalStrengthLeds '4'
	option signalStrengthLedOnValue '0'
	option signalStrengthLedOffValue '1'	
	option signalStrengthLed1 '496'
	option signalStrengthLed2 '497'
	option signalStrengthLed3 '498'
	option signalStrengthLed4 '499'
	option networkModeIndicatorType 'singleled'
	option highestNetworkMode '4g'
	option networkModeLedOnValue '0'
	option networkModeLedOffValue '1'	
	option networkModeLed '504'
		
config networkMode 'networkMode'
	option highestNetworkMode '4g'
	option SA '5g'
	option NSA '5g'
	option LTE '4g'
	option EDGE '3g'
	option GPRS '2g'
	option CDMA '2g'
	
config limit '5g'
	option limit1 '-115'
	option limit2 '-105'
	option limit3 '-95'
	
config limit '4g'
	option limit1 '-115'
	option limit2 '-105'
	option limit3 '-95'

config limit '3g'
	option limit1 '-100'
    option limit2 '-85' 
    option limit3 '-70'
 
config limit '2g'             
	option limit1 '-100'      
    option limit2 '-85'
    option limit3 '-70'
