
config system 'system'
	option hostname 'Silbo'
	option ttylogin '1'
	option urandom_seed '0'
	option zonename 'Asia/Kolkata'
	option timezone 'IST-5:30'
	option log_size '32'
	option conloglevel '8'
	option cronloglevel '5'
	option enablegps '0'
	option enablewifi '1'
	option ipkname 'Invendis'
	option model 'Silbo'
	option os 'Linux'
	option soc 'Mediatek'

config led 'led_wifi'
	option name 'wifi'
	option sysfs 'linkit-smart-7688:orange:wifi'
	option trigger 'phy0tpt'

config led 'led_lan1'
	option name 'lan1'
	option sysfs 'u7628-01:green:lan1'
	option trigger 'switch0'
	option port_mask '0x2'

config led 'led_lan2'
	option name 'lan2'
	option sysfs 'u7628-01:green:lan2'
	option trigger 'switch0'
	option port_mask '0x4'

config led 'led_lan3'
	option name 'lan3'
	option sysfs 'u7628-01:green:lan3'
	option trigger 'switch0'
	option port_mask '0x8'

config led 'led_lan4'
	option name 'lan4'
	option sysfs 'u7628-01:green:lan4'
	option trigger 'switch0'
	option port_mask '0x10'

config led 'led_wan'
	option name 'wan'
	option sysfs 'u7628-01:green:wan'
	option trigger 'switch0'
	option port_mask '0x01'

