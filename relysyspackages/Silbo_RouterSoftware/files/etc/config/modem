	option password ''
	option model 'EC200-A'
	option vendorid '2c7c'
	option productid '6005'
	option device '2'
	option comport '1'
	option smsport '0'
	option smsc '1'
	option monitorenable '1'
	option analyticsmanagerenable '1'
	option statusmanagerenable '1'
	option datatestenable '1'
	option pingtestenable '0'
	option dataenable '1'
	option apn 'airtelgprs.com'
	option ipv6 '1'
	option manufacturer 'Quectel'
	option porttype 'ttyUSB'
	option service 'auto'
	option pingip '8.8.8.8'
	option smsdeviceid '1'
	option smsapikey 'sdkgj'
	option actionmanagerenable '0'
	option metric '2'
	option modemenable '0'
