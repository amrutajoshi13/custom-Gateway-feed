
config login 'root'
        option username 'root'
        option password '$p$root'
        list read '*'
        list read '!ltemodulefirmwareupgrade'
        list read '!routerswinstalluninstallacl'
        list read '!testapp'
        list read '!uploadtestappacl'
         list read '!setprimarysim'
         list read '!watchdogconfig'
        list write '*'
        list write '!ltemodulefirmwareupgrade'
        list write '!routerswinstalluninstallacl'
        list write '!testapp'
        list write '!uploadtestappacl'
        list write '!setprimarysim'
        list write '!watchdogconfig'

