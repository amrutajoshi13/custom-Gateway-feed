
config interface 'cellularmodule'
        option singlesimdualmodule '0'
        option gpsenabled '0'
        option singlesimsinglemodule '1'
        option dualsimsinglemodule '0'

config interface 'CWAN1'
        option proto '3g'
        option loganalytics '1'
        option interfacename 'usb0'
        option maxifacedowninterval '270'
        option ifacequeryinterval '60'
        option querymodematanalytics '1'
        option packetcount '3'
        option pingdeadline '6'
        option minpacketloss '66'
        option action1enable '1'
        option action1cmd 'ModemRecycle'
        option action1retry '1'
        option action2enable '0'
        option action2cmd 'SimSwitch'
        option action2retry '1'
        option action2waitinterval '600'
        option action3enable '0'
        option action3cmd 'BoardRecycle'
        option action3retry '3'
        option action3waitinterval '600'
        option ifaceupsmsnotify '0'
        option ifacedownsmsnotify '0'
        option phonenumber '911234567890'
        option model 'EC200-A'
        option vendorid '2c7c'
        option productid '6005'
        option device '2'
        option comport '1'
        option monitorenable '1'
        option datatestenable '1'
        option pingtestenable '0'
        option dataenable '1'
        option apn 'airtelgprs.com'
        option ipv6 '1'
        option manufacturer 'Quectel'
        option porttype 'ttyUSB'
        option service 'auto'
        option pingip '8.8.8.8'
        option metric '2'
        option protocol 'cdcether'
        option smsservernumber '123'
        option smsport '1'
        option modemenable '1'
        option smsenable '1'
        option smsc '919845086007'
        option smsdeviceid 'AAAAAA41624'
        option smsapikey 'everestG1K'
        option actionmanagerenable '1'
        option analyticsmanagerenable '0'
        option statusmanagerenable '0'
        option smsresponseserverenable '1'
        option smsservernumber1 '91XXXXXXXX'
        option smsservernumber2 '91XXXXXXXX'
        option smsservernumber3 '12345678'

