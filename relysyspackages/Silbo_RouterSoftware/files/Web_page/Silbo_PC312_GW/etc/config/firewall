
config defaults 'def'
	option syn_flood '1'
	option input 'ACCEPT'
	option output 'ACCEPT'
	option forward 'ACCEPT'

config zone 'vpn'
	option name 'VPN'
	option input 'ACCEPT'
	option output 'ACCEPT'
	option forward 'ACCEPT'
	option mtu_fix '1'
	option masq '1'
	option network 'VPN'

config zone 'vpn1'
	option name 'VPN1'
	option input 'ACCEPT'
	option output 'ACCEPT'
	option forward 'ACCEPT'
	option mtu_fix '1'
	option masq '1'
	option network 'VPN1'

config rule 'ipsec_rule1'
	option name 'Allow-IKE-input'
	option src 'EWAN2'
	option proto 'udp'
	option dest_port '500'
	option target 'ACCEPT'

config rule 'ipsec_rule2'
	option name 'Allow-IKE-input'
	option src 'EWAN2'
	option proto 'udp'
	option dest_port '4500'
	option target 'ACCEPT'

config rule 'ipsec_rule3'
	option name 'Allow-ESP-input'
	option src 'EWAN2'
	option proto 'esp'
	option target 'ACCEPT'

config include
	option path '/etc/firewall.user'

config forwarding 'lan3vpn'
	option src 'LAN3'
	option dest 'VPN'

config forwarding 'vpnlan3'
	option src 'VPN'
	option dest 'LAN3'

config forwarding 'lan4vpn'
	option src 'LAN4'
	option dest 'VPN'

config forwarding 'vpnlan4'
	option src 'VPN'
	option dest 'LAN4'

config forwarding 'lan1vpn'
	option src 'LAN1'
	option dest 'VPN'

config forwarding 'vpnlan1'
	option src 'VPN'
	option dest 'LAN1'

config forwarding 'lan2vpn'
	option src 'LAN2'
	option dest 'VPN'

config forwarding 'vpnlan2'
	option src 'VPN'
	option dest 'LAN2'

config forwarding 'swlanvpn'
	option src 'SW_LAN'
	option dest 'VPN'

config forwarding 'vpnswlan'
	option src 'VPN'
	option dest 'SW_LAN'

config forwarding 'lan3vpn1'
	option src 'LAN3'
	option dest 'VPN1'

config forwarding 'vpn1lan3'
	option src 'VPN1'
	option dest 'LAN3'

config forwarding 'lan4vpn1'
	option src 'LAN4'
	option dest 'VPN1'

config forwarding 'vpn1lan4'
	option src 'VPN1'
	option dest 'LAN4'

config forwarding 'lan1vpn1'
	option src 'LAN1'
	option dest 'VPN1'

config forwarding 'vpn1lan1'
	option src 'VPN1'
	option dest 'LAN1'

config forwarding 'lan2vpn1'
	option src 'LAN2'
	option dest 'VPN1'

config forwarding 'vpn1lan2'
	option src 'VPN1'
	option dest 'LAN2'

config forwarding 'swlanvpn1'
	option src 'SW_LAN'
	option dest 'VPN1'

config forwarding 'vpn1swlan'
	option src 'VPN1'
	option dest 'SW_LAN'

config forwarding 'cwan1ra0'
	option src 'ra0'
	option dest 'CWAN1'

config forwarding 'EWANra0'
	option src 'ra0'
	option dest 'EWAN'

config forwarding 'ra0cwan1'
	option src 'CWAN1'
	option dest 'ra0'

config forwarding 'ra0EWAN'
	option src 'EWAN'
	option dest 'ra0'

config zone 'wifi'
	option name 'ra0'
	option input 'ACCEPT'
	option output 'ACCEPT'
	option forward 'ACCEPT'
	option network 'ra0'
	option masq '1'
	option mtu_fix '1'
	option extra_src '-m policy --dir in --pol none'
	option extra_dest '-m policy --dir out --pol none'

config zone 'lan'
	option name 'LAN'
	option input 'ACCEPT'
	option output 'ACCEPT'
	option forward 'ACCEPT'
	option network 'LAN'
	option masq '1'
	option mtu_fix '1'
	option extra_src '-m policy --dir in --pol none'
	option extra_dest '-m policy --dir out --pol none'

config zone 'ewan'
	option name 'EWAN'
	option input 'ACCEPT'
	option output 'ACCEPT'
	option forward 'ACCEPT'
	option network 'EWAN'
	option masq '1'
	option mtu_fix '1'
	option extra_src '-m policy --dir in --pol none'
	option extra_dest '-m policy --dir out --pol none'
	
config rule
	option name 'Block_Wifi'
	option family 'ipv4'
	option proto 'all'
	option src 'ra0'
	option dest 'EWAN'
	option target 'REJECT'	

