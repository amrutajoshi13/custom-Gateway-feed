
config interface 'loopback'
	option ifname 'lo'
	option proto 'static'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'

config interface 'LAN'
	option ifname 'eth0'
	option proto 'static'
	option netmask '255.255.255.0'
	option ipaddr '192.168.9.1'
    option dns '8.8.8.8'
    
config interface 'ra0'
	option ipaddr '192.168.90.1'
	option netmask '255.255.255.0'
	option proto 'static'
	option ifname 'ra0'



