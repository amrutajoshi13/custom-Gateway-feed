
config redirect 'SW_LAN'
        option interface 'SW_LAN'
        option ifname 'eth0.1'
        option type 'LAN'
        option staticIP '192.168.10.1'
        option staticnetmask '255.255.255.0'        
        option dhcpserver 'enable_dhcpv4v6'
        option ServerDHCPlimit '100'
        option ServerDHCPrange '50'
        option leasetime_duration 'h'
		option leasetimehr '12'
        option enable_zoneforward '1'
        option internetoverinterface '1'
        option protocol_lan 'static_lan'
        option ipv6addressingmode 'stateless'
        option ipmodelan '1'
        option ipmodelandelegateprefix '1'
        option ipv6delegateprefix '64'
        option ipv6prefixhint '12a'
        option ipv6interval '1'
        option ra_mininterval '30'
        option ra_maxinterval '60'
        option ipv6metric '121'
        option custom_DHCP_options '1'
        option enable_dns '1' 
        list ServerStaticDnsServer '8.8.8.8'

