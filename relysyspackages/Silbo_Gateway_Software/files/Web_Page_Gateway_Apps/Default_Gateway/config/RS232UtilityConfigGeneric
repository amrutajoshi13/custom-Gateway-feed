
config RS232UtilityConfigGeneric 'rs232utilityconfig'
	option ModbusProtocol 'RTU'
	option Stopbits '1'
	option Databits '8'
	option Baudrate '9600'
	option StartRegister '0'
	option NumberOfRegisters '68'
	option FunctionalCode '4'
	option Parity '2'
	option flow_control '0'
	option SerialPort '/dev/ttyS2'
	option delay '50'

