
config cloudconfig 'cloudconfig'
	option EnableSecondaryServer '0'
	option HTTPMethod '0'
	option mqttusername 'admin'
	option mqttpassword 'admin'
	option enabledelete '1'
	option host 'broker.hivemq.com'
	option mqttauthmode '2'
	option cloudprotocol 'http'
	option HTTPServerURL 'www.httpbin.org/post'
	option httpauthenable '0'
	option username 'admin'
	option password 'admin'
	option serverresponsestring 'Recordid'
	option serverresponsevalidationenable '0'
	option EMeterRS485Line1DataSourceEnable '1'
    option EMeterRS485Line2DataSourceEnable '0'
    option EMeterRS232Line1DataSourceEnable '0'
    option DIODataSourceEnable '1'
    option AIODataSourceEnable '1'
    option TEMPERATUREDataSourceEnable '1'
