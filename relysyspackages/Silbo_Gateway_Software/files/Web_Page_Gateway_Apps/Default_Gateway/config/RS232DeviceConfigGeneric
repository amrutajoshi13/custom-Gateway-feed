
config RS232Config 'DeepSea1'
	option EEnable '0'
	option StartRegister '99'
	option Baudrate '9600'
	option NoOfStopbits '1'
	option Databits '8'
	option FunctionalCode '3'
	option ModbusProtocol 'RTU'
	option InterfaceID '1'
	option MeterID '1'
	option NoOfRegister '10'
	option SlaveAddress '1'
	option USBFilename '/dev/ttyS2'
	option Model 'DeepSea7320'
	option Parity '0'
	option FlowControl '0'
	option Delay '500'

config RS232Config 'DeepSea2'
	option EEnable '0'
	option InterfaceID '1'
	option MeterID '1'
	option Model 'Deepasea'
	option StartRegister '155'
	option NoOfRegister '10'
	option Baudrate '9600'
	option NoOfStopbits '1'
	option Databits '8'
	option FunctionalCode '3'
	option ModbusProtocol 'RTU'
	option Parity '0'
	option USBFilename '/dev/ttyS2'
	option FlowControl '0'
	option Delay '500'
	option SlaveAddress '1'

