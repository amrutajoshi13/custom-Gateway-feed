config IOUtilities
    option utility "MODBUS UTILITY"
    option action "READORWRITE"
    option inputs "1"
    option input1Name "Slave ID"
    option input1Element "text"
    option input1Type "text"  
    
config IOUtilities
    option utility "RS232 Device"
    option action "READ"
    option inputs "1"
    option input1Name "Slave ID"
    option input1Element "text"
    option input1Type "text"    

config IOUtilities                          
        option utility 'Analog Input'
        option action 'READ'                
        option inputs '3'               
        option input1Element 'selection'
        option input1Name 'AIO'               
        option input1Type 'string'   
        list input1Values '1'                 
        list input1Values '2'  
        option input2Name 'Range 1-6 - default: 2'
        option input2Element 'text'               
        option input2Type 'text'
    
 
config IOUtilities 
	option action 'READ'
	option input1Element 'selection'
	option input1Name 'DIO'
	option input1Type 'string'
	list input1Values '1'
	list input1Values '2'
	list input1Values '3'
	list input1Values '4'
	option inputs '2'
	option utility 'Digital Input'
	
config IOUtilities                          
        option utility 'Digital Output'
        option action 'WRITE'                
        option inputs '2'               
        option input1Element 'selection'
        option input1Name 'DO'               
        option input1Type 'string'   
        list input1Values '1'                 
        list input1Values '2' 
        list input1Values '3'
        list input1Values '4' 
        option input2Name 'value 0 or 1'
        option input2Element 'text'               
        option input2Type 'text'	
        
config IOUtilities                          
        option utility 'Delete Database'
        option action 'DELETE'                
        option inputs '1'               
        option input1Element 'selection'
        option input1Name 'Database/File'               
        option input1Type 'string'   
        list input1Values 'EnergyMeterRS485'
        list input1Values "DIO"
        list input1Values "AIO"
		list input1Values "EnergyMeterRS232"
		list input1Values "SNMP"
		list input1Values "Temperature"

       

        
	
config IOUtilities                          
        option utility 'Copy Database to USB'
        option action 'COPY'                
        option inputs '1'               
        option input1Element 'selection'
        option input1Name 'Database/File'               
        option input1Type 'string'   
        list input1Values 'EnergyMeterRS485'
        list input1Values "DIO"
        list input1Values "AIO"
		list input1Values "EnergyMeterRS232"
